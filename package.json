{
  "name": "@jihyunlab/web-secure-storage",
  "version": "2.0.0",
  "description": "JihyunLab Web secure storage.",
  "license": "(MIT OR Apache-2.0)",
  "author": "JihyunLab <info@jihyunlab.com> (https://jihyunlab.com)",
  "homepage": "https://jihyunlab.com/library/npm/web-secure-storage",
  "repository": "https://github.com/jihyunlab/web-secure-storage",
  "bugs": "https://github.com/jihyunlab/web-secure-storage/issues",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:wasm": "wasm-pack build --target web --out-dir src/wasm",
    "build": "wasm-pack build --target web --out-dir dist/wasm && tsc --build",
    "start": "wasm-pack build --target web --out-dir dist/wasm && tsc --build && node dist/index.js",
    "clean": "tsc --build --clean",
    "lint": "eslint src/**/*.ts && eslint test/**/*.ts",
    "test": "jest --coverage --coverageReporters=cobertura"
  },
  "keywords": [
    "web-secure-storage",
    "web",
    "secure",
    "storage",
    "rust",
    "wasm",
    "typescript"
  ],
  "engines": {
    "node": ">=16"
  },
  "dependencies": {},
  "devDependencies": {
    "@jihyunlab/eslint-config": "^1.1.3",
    "@jihyunlab/prettier-config": "^1.1.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
